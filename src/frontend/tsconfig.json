{
  "compilerOptions": {
    "jsx": "react",
    "target": "ES6",
    "moduleResolution": "node",
    "strict": true,
    "noImplicitAny": true,
    "resolveJsonModule": true,
    "typeRoots": [
      "node_modules/@types",
      "src/Utils/types"
    ],
    "allowJs": true,
    "suppressImplicitAnyIndexErrors": true,
    "allowSyntheticDefaultImports": true
  }
}